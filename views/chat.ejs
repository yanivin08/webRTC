<div class="container">
    <div class="row mx-1" >
        <div class="col-md-8">
            <h3 class="mt-3 text-center"><i class="fab fa-battle-net"></i> Web-RTC Sessions</h3>
        </div>
        <div class="col-md-4">
            <strong>  
                <h4 id="usernames" class="mt-3 row">
                    <%= name %>
                </h4>
            </strong>
            <small class="mb-3 row">March 26, 2020</small>
        </div>
    </div>
    <div class="row">
        <div class="col-md-8">
        </div>
        <div class="col-md-4">
            <div class="row mx-1" style="height: 270px">
                <h6><strong><i class="fas fa-camera-retro mr-2"></i>Webcam</strong></h6>
            </div>
            <div class="row mx-1 mb-3">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h6><strong><i class="fas fa-comment-dots mr-2"></i>Chat</strong></h6>
                    </div>
                    <div class="panel-body">
                        <ul class="chat list-group list-unstyled chat-messages"></ul>
                    </div>
                    <div class="panel-footer">
                        <div class="input-group">
                            <form id="chat-form"  class="input-group mt-3">
                                <input id="msg" type="text" class="form-control input-sm" placeholder="Type your message here...">
                                <span class="input-group-btn">
                                    <button class="btn btn-warning btn-sm" id="btn-chat">Send</button>
                                </span>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>    
<script src="/socket.io/socket.io.js"></script>
<script src="/js/main.js"></script>